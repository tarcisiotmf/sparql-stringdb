###  http://www.geneontology.org/formats/oboInOwl#hasExactSynonym
oboInOwl:hasExactSynonym rdf:type owl:AnnotationProperty ;
                         rdfs:comment "An alias in which the alias exhibits true synonymy. Example: ornithine cycle is an exact synonym of urea cycle"@en ;
                         rdfs:label "has_exact_synonym"@en ;
                         rdfs:subPropertyOf oboInOwl:hasSynonym  ;
                rdfs:isDefinedBy oboInOwl: .

###  http://www.geneontology.org/formats/oboInOwl#hasSynonym
oboInOwl:hasSynonym rdf:type owl:AnnotationProperty ;
                    rdfs:comment "A relation between a class and an alias term."@en ;
                    rdfs:comment "Note that this should be a super-property of hasRelatedSynonym, hasExactSynonym etc; however, we cannot state this and remain in OWL-DL"^^xsd:string ;
                    rdfs:label "has_synonym"@en  ;
                    rdfs:isDefinedBy oboInOwl: .

###  http://www.geneontology.org/formats/oboInOwl#hasAlternativeId
oboInOwl:hasAlternativeId rdf:type owl:AnnotationProperty ;
                          rdfs:label "has_alternative_id"^^xsd:string  ;
                rdfs:isDefinedBy oboInOwl: .


###  http://www.geneontology.org/formats/oboInOwl#hasDbXref
oboInOwl:hasDbXref rdf:type owl:AnnotationProperty ;
                   rdfs:label "has_dbxref"@en ;
                rdfs:isDefinedBy oboInOwl: .


oboInOwl:hasRelatedSynonym rdf:type owl:AnnotationProperty ;
                           rdfs:comment   "An alias in which the alias is related the primary class name, but not necessarily broader or narrower. Example: cytochrome bc1 complex is a related synonym of ubiquinol-cytochrome-c reductase activity; virulence is a related synonym of pathogenesis"@en ;
                           rdfs:label "has_related_synonym"@en ;
                           rdfs:subPropertyOf oboInOwl:hasSynonym ;
                rdfs:isDefinedBy oboInOwl: .

:functionallyInteractsWith rdf:type owl:ObjectProperty, owl:SymmetricProperty, owl:IrreflexiveProperty;
	  rdfs:subPropertyOf obo:RO_0002434 ;
	  rdfs:label "functionally interacts with"^^xsd:string ;
      rdfs:domain orth:Protein ;
      rdfs:range  orth:Protein ;
      rdfs:comment "It states a protein that functionally interacts with another one, a pairwise relationship."^^xsd:string .

:physicallyInteractsWith rdf:type owl:ObjectProperty, owl:SymmetricProperty, owl:IrreflexiveProperty;
	  rdfs:subPropertyOf obo:RO_0002434 ;
	  rdfs:label "physically interacts with"^^xsd:string ;
      rdfs:domain orth:Protein ;
      rdfs:range  orth:Protein ;
      rdfs:comment "It states a protein that physically interacts with another one, a pairwise relationship."^^xsd:string .

:ProteinProteinInteraction rdf:type owl:Class ;
		rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasProtein ;
                           owl:someValuesFrom orth:Protein ;
                           owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger 
                         ] ;
		skos:closeMatch <http://purl.obolibrary.org/obo/NCIT_C18469>;
		rdfs:comment "Temporary, non-covalent binding between protein molecules. Protein-protein interactions occur as a result of intermolecular physical forces and spatial complementation between domains or motifs. This interaction can be either homotypic or heterotypic and effect protein structure, conformation, and function.[source: NCIT]"^^xsd:string ;
		rdfs:label "protein-protein interaction"^^xsd:string ;
		skos:altLabel "PPI"^^xsd:string.

:hasFunctionalInteractionScore rdf:type owl:ObjectProperty, owl:FunctionalProperty;
			rdfs:range :Score;
			rdfs:domain :ProteinProteinInteraction ;
			rdfs:label "has functional interaction score"^^xsd:string;
			rdfs:comment "It states a protein-protein functional interaction score."^^xsd:string.


:hasPhysicalInteractionScore rdf:type owl:ObjectProperty, owl:FunctionalProperty;
			rdfs:range :Score;
			rdfs:domain :ProteinProteinInteraction ;
			rdfs:label "has physical interaction score"^^xsd:string;
			rdfs:comment "It states a protein-protein physical interaction score."^^xsd:string.


:Score rdf:type owl:Class;
		rdfs:label "score"^^xsd:string;
		rdfs:comment "Class to define a set of any type of score values."^^xsd:string.


:hasProtein rdf:type owl:ObjectProperty;
			rdfs:range orth:Protein;
			rdfs:domain [ rdf:type owl:Class ;
                    owl:unionOf ( :ProteinProteinInteraction
                                  :PPIEnrichment  )
                                     ] ;  
			rdfs:label "has protein"^^xsd:string ;
			rdfs:comment "It states a protein that is part of or contained in the subject."^^xsd:string.

:combinedScore	rdf:type owl:DatatypeProperty, owl:FunctionalProperty ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;				
                rdfs:label "combined score"^^xsd:string ;
                rdfs:comment "The combined score is computed by combining the probabilities from the different evidence channels  and corrected for the probability of randomly observing an interaction. The combined score is computed from the partial scores (e.g. fusion score, experimental score)."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "score"^^xsd:string.

:neighborhoodScore	rdf:type owl:DatatypeProperty , owl:FunctionalProperty ; 
				rdfs:subPropertyOf :partialScore ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;			
                rdfs:label "gene neighborhood score"^^xsd:string ;
                rdfs:comment "The neighborhood score is computed from the inter-gene nucleotide count."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "nscore"^^xsd:string.


:fusionScore	rdf:type owl:DatatypeProperty , owl:FunctionalProperty ; 
				rdfs:subPropertyOf :partialScore ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;			
                rdfs:label "gene fusion score"^^xsd:string ;
                rdfs:comment "The gene fusion score is derived from fused proteins in other species."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "fscore"^^xsd:string.

:phylogeneticScore	rdf:type owl:DatatypeProperty , owl:FunctionalProperty ; 
				rdfs:subPropertyOf :partialScore ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;			
                rdfs:label "phylogenetic profile score"^^xsd:string ;
                rdfs:comment "The cooccurence score of the phyletic profile that is derived from similar absence/presence patterns of genes."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "pscore"^^xsd:string.

:coexpressionScore	rdf:type owl:DatatypeProperty , owl:FunctionalProperty ; 
				rdfs:subPropertyOf :partialScore ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;			
                rdfs:label "coexpression score"^^xsd:string ;
                rdfs:comment "The coexpression score is derived from similar pattern of mRNA expression measured by DNA arrays and similar technologies."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "ascore"^^xsd:string.

:experimentalScore	rdf:type owl:DatatypeProperty , owl:FunctionalProperty ; 
				rdfs:subPropertyOf :partialScore ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;			
                rdfs:label "experimental score"^^xsd:string ;
                rdfs:comment "The experimental score is derived from experimental data, such as, affinity chromatography."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "escore"^^xsd:string.

:databaseScore	rdf:type owl:DatatypeProperty , owl:FunctionalProperty ; 
				rdfs:subPropertyOf :partialScore ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;			
                rdfs:label "database score"^^xsd:string ;
                rdfs:comment "The database score is derived from curated data of various databases."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "dscore"^^xsd:string.

:textMiningScore rdf:type owl:DatatypeProperty , owl:FunctionalProperty ; 
				rdfs:subPropertyOf :partialScore ;
				rdfs:range xsd:double;
				rdfs:domain :Score ;			
                rdfs:label "text mining score"^^xsd:string ;
                rdfs:comment "The textmining score is derived from the co-occurrence of gene/protein names in abstracts."^^xsd:string;
                rdfs:seeAlso <http://www.ncbi.nlm.nih.gov/pubmed/15608232>;
                skos:altLabel "tscore"^^xsd:string.

:partialScore  rdf:type owl:DatatypeProperty ; 
				rdfs:range xsd:double;
				rdfs:domain :Score ;
				rdfs:label "partial score"^^xsd:string ;
                rdfs:comment "The partial scores used to compute the combined scores."^^xsd:string.
####
#### Terms added after considering STRING Web API: "Retrieving similarity scores of the protein set" and "Retrieving best similarity hits between species"
####

###  http://purl.org/net/orth#HomologyRelation
orth:HomologyRelation rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty orth:hasHomologousMember ;
                                        owl:allValuesFrom orth:SequenceUnit
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty orth:inDataset ;
                                        owl:allValuesFrom orth:OrthologyDataset
                                      ] ;
                      rdfs:comment "This class represents homology relations among sequence units (e.g genes, proteins or gene regions)."@en ;
                      rdfs:label "homology relation"@en ;
                      <http://www.w3.org/2004/02/skos/core#altLabel> "homologous relation"@en .

###  http://purl.org/net/orth#hasHomologousMember
orth:hasHomologousMember rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf sio:SIO_000028 ;
                         owl:inverseOf orth:isHomologousMember ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( orth:HomologsCluster
                                                     orth:HomologyRelation
                                                   )
                                     ] ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( orth:HomologsCluster
                                                    orth:SequenceUnit
                                                  )
                                    ] ;
                         rdfs:comment "Property that permits to define the membership of a cluster of homologs or a sequence unit to a clusters of homologs."@en ;
                         rdfs:label "has homologous member"@en .

###  http://purl.org/net/orth#isHomologousMember
orth:isHomologousMember rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf obo:RO_0002350 ;
                        rdfs:comment "Inverse of has homologous member. This property permits to find the cluster in which a cluster or sequence unit is included."@en ;
                        rdfs:label "is homologous member"@en .

###  http://purl.obolibrary.org/obo/RO_0002350
obo:RO_0002350 rdf:type owl:ObjectProperty ;
               rdfs:comment "Is member of is a mereological relation between a item and a collection."@en ;
               rdfs:isDefinedBy "http://purl.obolibrary.org/obo/ro.owl" ;
               rdfs:label "member of"@en .

###  http://semanticscience.org/resource/SIO_000028
sio:SIO_000028 rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ,
                        owl:ReflexiveProperty ;
               rdfs:comment "has part is a transitive, reflexive and antisymmetric relation between a whole and itself or a whole and its part."@en ;
               rdfs:isDefinedBy "http://semanticscience.org/ontology/sio.owl" ;
               rdfs:label "has part"@en .

###  http://purl.obolibrary.org/obo/CDAO_0000178
obo:CDAO_0000178 rdf:type owl:ObjectProperty ;
                 owl:inverseOf obo:CDAO_0000194 ;
                 rdfs:comment "Generic 'has' property."@en ;
                 rdfs:label "has"^^xsd:string .

###  http://purl.org/net/orth#hasHomologyData
orth:hasHomologyData rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf obo:CDAO_0000178 ;
                     owl:inverseOf orth:inDataset ;
                     rdfs:domain orth:OrthologyDataset ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( orth:HomologsCluster
                                                orth:HomologyRelation
                                              )
                                ] ;
                     rdfs:comment "Inverse property of inDataset.  It permits to retrieve  the clusters or homology relations included in a particular dataset."@en ;
                     rdfs:label "has homology data"@en .

###  http://purl.org/net/orth#inDataset
orth:inDataset rdf:type owl:ObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( orth:HomologsCluster
                                           orth:HomologyRelation
                                         )
                           ] ;
               rdfs:range orth:OrthologyDataset ;
               rdfs:comment "Property that provides the dataset associated with a gene tree node"@en ;
               rdfs:label "in dataset"@en .    

###  http://purl.org/net/orth#HomologsCluster
orth:HomologsCluster rdf:type owl:Class ;
                     rdfs:comment "Set of homologous sequences"@en ;
                     rdfs:label "Cluster of homologous sequences"@en ;
                     <http://www.w3.org/2004/02/skos/core#altLabel> "Cluster of homologs"@en ,
                                                                    "homologous group"@en ,
                                                                    "homologs cluster"@en .

:bitscore rdf:type owl:DatatypeProperty , owl:FunctionalProperty ;
				rdfs:range xsd:double;
				rdfs:domain orth:HomologyRelation ;			
                rdfs:label "bit-score"^^xsd:string ;
                rdfs:comment "Smith-Waterman alignment bit-score."^^xsd:string;
                rdfs:seeAlso <https://en.wikipedia.org/wiki/Smithâ€“Waterman_algorithm>;
                skos:altLabel "bitscore"^^xsd:string.

###  http://purl.org/net/orth#hasHomolog
orth:hasHomolog rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf obo:CDAO_0000178 ;
                rdf:type owl:SymmetricProperty ;
                rdfs:domain orth:SequenceUnit ;
                rdfs:range orth:SequenceUnit ;
                rdfs:comment "Homologous pairwise relation between sequence units (e.g.: genes, proteins or gene regions). For example, A Gene(A) hasHomolog Gene(B). This property can be materialized or inferred by applying a logical rule."@en ;
                rdfs:label "has homolog"@en ;
                <http://www.w3.org/2004/02/skos/core#altLabel> "is homologous to"@en .

###  http://purl.obolibrary.org/obo/CDAO_0000178
obo:CDAO_0000178 rdf:type owl:ObjectProperty ;
                 owl:inverseOf obo:CDAO_0000194 ;
                 rdfs:comment "Generic 'has' property."@en ;
                 rdfs:label "has"^^xsd:string .

###  http://purl.obolibrary.org/obo/CDAO_0000194
obo:CDAO_0000194 rdf:type owl:ObjectProperty ;
                 rdfs:label "part_of"^^xsd:string .


####
#### Terms added after considering STRING Web API: Getting protein-protein interaction enrichment
####             
###  http://purl.obolibrary.org/obo/OBI_0000175
obo:OBI_0000175 rdf:type owl:Class ;
                obo:IAO_0000111 "p-value"@en ;
                obo:IAO_0000112 """PMID:19696660
in contrast to the in-vivo data AT-III increased significantly from
113.5% at baseline to 117% after 4 days (n = 10, P-value= 0.02; Table 2)."""^^xsd:string ;
                obo:IAO_0000115 "A quantitative confidence value that represents the probability of obtaining a result at least as extreme as that actually obtained, assuming that the actual value was the result of chance alone."@en ;
                obo:IAO_0000119 "WEB: http://en.wikipedia.org/wiki/P-value"^^xsd:string ;
                rdfs:label "p-value"@en ;
                rdfs:isDefinedBy <http://purl.obolibrary.org/obo/obi/2020-04-23/obi.owl> ;
                skos:inScheme <http://purl.obolibrary.org/obo/obi.owl> ;
                terms:license <http://creativecommons.org/licenses/by/4.0/> .

:pvalue a owl:DatatypeProperty, owl:FunctionalProperty ;
		skos:relatedMatch <http://purl.obolibrary.org/obo/OBI_0000175>;
		rdfs:comment "P-value for statistical significance. For example, in STRING DB, the significance of the network having more interactions than expected.";
		rdfs:label "p-value";
		rdfs:range xsd:double ;
		rdfs:domain :PPIEnrichment. 


:nodeNumber a owl:DatatypeProperty, owl:FunctionalProperty ;
		rdfs:comment "The number of proteins in the network, that are representend with nodes.";
		rdfs:label "protein amount";
		rdfs:range xsd:nonNegativeInteger ;
		rdfs:domain :PPIEnrichment. 	


:edgeNumber	a owl:DatatypeProperty, owl:FunctionalProperty ;
		rdfs:comment "The number of edges in the network.";
		rdfs:label "edge amount";
		rdfs:range xsd:nonNegativeInteger ;
		rdfs:domain :PPIEnrichment. 	

:averageNodeDegree a owl:DatatypeProperty, owl:FunctionalProperty ;
		rdfs:comment "The mean degree of the node in the network.";
		rdfs:label "average node degree";
		rdfs:range xsd:double ;
		rdfs:domain :PPIEnrichment. 

:localClusteringCoefficient a owl:DatatypeProperty, owl:FunctionalProperty ;
		rdfs:comment "The average local clustering coefficient.";
		rdfs:label "edge amount";
		rdfs:range xsd:double ;
		rdfs:domain :PPIEnrichment. 

:expectedEdgeNumber a owl:DatatypeProperty, owl:FunctionalProperty ;
		rdfs:comment "The expected number of edges based on the nodes degrees.";
		rdfs:label "expected edge amount";
		rdfs:range xsd:nonNegativeInteger ;
		rdfs:domain :PPIEnrichment.

:PPIEnrichment rdf:type owl:Class;
		rdfs:comment "Even in the absence of annotated proteins (e.g. in novel genomes) databases such as STRING can tell if the subset of proteins is functionally related, that is, if it is enriched in interactions in comparison to the background proteome-wide interaction distribution.";
		rdfs:label "protein-protein interaction enrichment".

